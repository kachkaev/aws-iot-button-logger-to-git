  "scripts": {
    "build": "ts-node src/build.ts",
    "fix": "npm-run-all --continue-on-error fix:*",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "simulate": "ts-node src/simulate.ts",
    "test": "jest"
  },
  "dependencies": {
    "@kachkaev/commands": "^0.1.1",
    "dotenv": "^10.0.0",
    "envalid": "^7.2.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "jszip": "^3.7.1",
    "luxon": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@kachkaev/markdownlint-config": "^0.2.0",
    "@kachkaev/prettier-config": "^1.2.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/jszip": "^3.4.1",
    "@types/luxon": "^2.0.7",
    "@types/node": "^16.11.7",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "markdownlint-cli": "^0.29.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "prettier": "^2.4.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "yarn-deduplicate": "^3.1.0"
  },
  "engines": {
    "node": ">=12.21.0",
    "yarn": ">=1.22.0"
  }
}
